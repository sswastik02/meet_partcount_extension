{
    "name": "Countpart",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Counting and checking participant",
    "background": {
      "scripts": [
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_title": "Inject!"
    },
    "permissions": [
      "https://*/*",
      "http://*/*",
      "tabs",
      "activeTab",
      "storage"
    ]
  }